<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Rhetos.AspNetFormsAuth</id>
    <version>5.0.0-dev</version>
    <authors>Omega software</authors>
    <owners>Omega software</owners>
    <licenseUrl>http://opensource.org/licenses/AGPL-3.0</licenseUrl>
    <projectUrl>https://github.com/rhetos/rhetos</projectUrl>
    <description>AspNetFormsAuth is a plugin package for Rhetos development platform.
      It provides an implementation of ASP.NET forms authentication to Rhetos server applications.</description>
    <copyright>Copyright Omega software 2015</copyright>
    <tags>Rhetos plugin</tags>
    <dependencies>
      <dependency id="Rhetos.CommonConcepts" version="5.0.0-dev" />
      <dependency id="Rhetos.RestGenerator" version="5.0.0-dev" />
      <dependency id="Rhetos.Host.AspNet" version="5.0.0-dev" />
    </dependencies>
    <frameworkReferences>
      <group targetFramework="net5.0">
        <frameworkReference name="Microsoft.AspNetCore.App" />
      </group>
    </frameworkReferences>
    <contentFiles>
      <files include="any/any/AdminSetup.exe" buildAction="None" copyToOutput="true" />
      <files include="any/any/AdminSetup.dll" buildAction="None" copyToOutput="true" />
      <files include="any/any/AdminSetup.pdb" buildAction="None" copyToOutput="true" />
      <files include="any/any/AdminSetup.deps.json" buildAction="None" copyToOutput="true" />
      <files include="any/any/AdminSetup.runtimeconfig.json" buildAction="None" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="Readme.md" target="" />
    <file src="ChangeLog.md" target="" />
    <file src="src\Rhetos.AspNetFormsAuth\DataMigration\**\*" target="DataMigration" />
    <file src="src\Rhetos.AspNetFormsAuth\DslScripts\**\*" target="DslScripts" />
    <file src="src\Rhetos.AspNetFormsAuth\bin\Debug\net5.0\Rhetos.AspNetFormsAuth.dll" target="lib\net5.0" />
    <file src="src\Rhetos.AspNetFormsAuth\bin\Debug\net5.0\Rhetos.AspNetFormsAuth.pdb" target="lib\net5.0" />

    <file src="src\AdminSetup\bin\Debug\net5.0\AdminSetup.exe" target="contentFiles/any/any" />
    <file src="src\AdminSetup\bin\Debug\net5.0\AdminSetup.dll" target="contentFiles/any/any" />
    <file src="src\AdminSetup\bin\Debug\net5.0\AdminSetup.pdb" target="contentFiles/any/any" />
    <file src="src\AdminSetup\bin\Debug\net5.0\AdminSetup.deps.json" target="contentFiles/any/any" />
    <file src="src\AdminSetup\bin\Debug\net5.0\AdminSetup.runtimeconfig.json" target="contentFiles/any/any" />
  </files>
</package>
